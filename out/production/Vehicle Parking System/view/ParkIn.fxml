<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="context" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ParkInController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="602.0" layoutX="2.0" pickOnBounds="true">
         <image>
            <Image url="@../accets/2880529.jpg" />
         </image>
      </ImageView>
      <JFXButton fx:id="btnVehicle" layoutX="362.0" layoutY="58.0" onAction="#btnVehicleOnAction" style="-fx-background-color: #0652DD;" text="Add Vehicle">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnDriver" layoutX="490.0" layoutY="58.0" onAction="#btnDriverOnAction" style="-fx-background-color: #0652DD;" text="Add Driver">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXButton>
      <TableView fx:id="tblVehicle" layoutX="23.0" layoutY="158.0" prefHeight="200.0" prefWidth="563.0" style="-fx-background-color: A6A6A6;">
        <columns>
          <TableColumn fx:id="colNumber" prefWidth="106.0" text="Vehicle Num" />
          <TableColumn fx:id="colType" prefWidth="149.0" text="Vehicle Type" />
            <TableColumn fx:id="colSlot" prefWidth="154.0" text="Parking Slot" />
            <TableColumn fx:id="colTime" prefWidth="152.0" text="Parked Time" />
        </columns>
      </TableView>
      <JFXButton fx:id="btnDelivery" layoutX="23.0" layoutY="60.0" onAction="#btnDeliveryOnAction" prefHeight="25.0" prefWidth="158.0" style="-fx-background-color: #0652DD;" text="On Delivery">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </JFXButton>
      <Button fx:id="btnHome" layoutX="-3.0" layoutY="-2.0" mnemonicParsing="false" onAction="#btnHomeOnAction" prefHeight="32.0" prefWidth="54.0" text="Home">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
   </children>
</AnchorPane>
