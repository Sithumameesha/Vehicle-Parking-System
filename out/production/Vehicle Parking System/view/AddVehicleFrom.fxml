<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="context" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddVehicleFromController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true">
         <image>
            <Image url="@../accets/2880529.jpg" />
         </image>
      </ImageView>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="283.0" layoutX="109.0" layoutY="57.0" stroke="#0f3162" strokeType="INSIDE" width="382.0" />
      <JFXTextField fx:id="txtNum" labelFloat="true" layoutX="137.0" layoutY="78.0" prefHeight="25.0" prefWidth="200.0" promptText="Vehicle Number">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></JFXTextField>
      <JFXTextField fx:id="txtType" labelFloat="true" layoutX="137.0" layoutY="143.0" prefHeight="25.0" prefWidth="200.0" promptText="Vehicle Type">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></JFXTextField>
      <JFXTextField fx:id="txtPassenger" labelFloat="true" layoutX="137.0" layoutY="264.0" prefHeight="25.0" prefWidth="200.0" promptText="No oF Passenger">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></JFXTextField>
      <JFXTextField fx:id="txtWeight" labelFloat="true" layoutX="137.0" layoutY="200.0" prefHeight="25.0" prefWidth="200.0" promptText="Max Weight">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></JFXTextField>
      <JFXButton fx:id="btnAdd" layoutX="399.0" layoutY="168.0" onAction="#btnAddOnAction" prefHeight="25.0" prefWidth="86.0" style="-fx-background-color: #3498db;" text="Add">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnHome" layoutX="542.0" layoutY="2.0" onAction="#btnHomeOnAction" style="-fx-background-color: white;" text="Home">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnBack" layoutY="2.0" onAction="#btnBackOnAction" style="-fx-background-color: white;" text="Back">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnNew" layoutX="399.0" layoutY="210.0" onAction="#btnNewOnAction" prefHeight="25.0" prefWidth="86.0" style="-fx-background-color: #3498db;" text="New">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
