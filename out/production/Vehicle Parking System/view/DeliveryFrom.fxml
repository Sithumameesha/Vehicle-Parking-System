<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="context" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.DeliveryFromController">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" layoutY="-1.0" pickOnBounds="true">
         <image>
            <Image url="@../accets/2880529.jpg" />
         </image></ImageView>
      <JFXButton fx:id="btnVehicle" layoutX="369.0" layoutY="22.0" onAction="#btnVehicleOnAction" style="-fx-background-color: #0652DD;" text="Add Vehicle">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="btnDriver" layoutX="501.0" layoutY="22.0" onAction="#btnDriverOnAction" style="-fx-background-color: #0652DD;" text="Add Driver">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </JFXButton>
      <TableView fx:id="tblVehicleDelivery" layoutX="23.0" layoutY="127.0" onSort="#tblVehicleDeliveryOnAction" prefHeight="200.0" prefWidth="563.0">
         <columns>
            <TableColumn fx:id="colNumber" prefWidth="106.0" text="Vehicle Num" />
            <TableColumn fx:id="colType" prefWidth="149.0" text="Vehicle Type" />
            <TableColumn fx:id="colDriverName" prefWidth="154.0" text="Driver Name" />
            <TableColumn fx:id="colLeft" prefWidth="152.0" text="Left Time" />
         </columns>
      </TableView>
      <Button layoutX="23.0" layoutY="24.0" mnemonicParsing="false" text="Exit" />
   </children>
</AnchorPane>
